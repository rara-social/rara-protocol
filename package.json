{
  "name": "protocol",
  "version": "1.0.0",
  "main": "index.js",
  "license": "GPL-3.0",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test --network hardhat",
    "chain": "npx hardhat node --network hardhat --no-deploy",
    "coverage": "npx hardhat coverage --network hardhat",
    "deploy": "npx hardhat deploy --network localhost --export-all ./deploy_data/hardhat_contracts.json --tags localhost --reset",
    "run-graph-docker": "cd services/graph-node && docker-compose up",
    "stop-graph-docker": "cd services/graph-node && docker-compose down",
    "clean-graph-docker": "rm -rf services/graph-node/data/",
    "graph-codegen": "cd subgraph && yarn codegen",
    "graph-build": "cd subgraph && yarn build",
    "graph-deploy-local": "cd subgraph && yarn deploy-local"
  },
  "devDependencies": {
    "@manifoldxyz/royalty-registry-solidity": "^1.0.9",
    "@maticnetwork/fx-portal": "^1.0.4",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.4.2",
    "@openzeppelin/hardhat-upgrades": "^1.13.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.8.3",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.10.5",
    "hardhat-gas-reporter": "^1.0.4",
    "mustache": "^4.2.0",
